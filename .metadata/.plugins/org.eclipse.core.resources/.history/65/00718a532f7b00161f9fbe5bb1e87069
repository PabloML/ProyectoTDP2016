package Graphics.Background;

import javax.swing.JPanel;
import javax.swing.JLabel;
import javax.swing.ImageIcon;

public class MapPanel extends JPanel {
	public JLabel player;
	/**
	 * Create the panel.
	 */
	public MapPanel()
	  {
		setLayout(null);
		player = new JLabel("");
		player.setIcon(new ImageIcon(MapPanel.class.getResource("/Images/Base.png")));
		player.setBounds(202, 5, 64, 56);
		add(player);
	  }

}
